<launch>

  <!-- the ROS parameter /use_sim_time
  triggers ROS time to use the simulation time in the 
  /clock ROS topic published by gazebo rather than 
  the system wall clock time -->
  <param name="/use_sim_time" value="true" />

	<arg name="model" default="$(find hubo_description)/huboplus/huboplus.xml" />
	
    <param name="robot_description" textfile="$(arg model)" />
	<node name="hubo_joint_state_publisher" pkg="hubo_ach_ros_bridge" type="hubo_joint_state_publisher.py" output="screen"></node>
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />

</launch>
